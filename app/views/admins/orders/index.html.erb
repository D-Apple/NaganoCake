<div class="container">     
      <h2>注文履歴一覧</h2>
      <table class="table">
        <thead class="thead-light">
          <tr>
            <th>購入日時</th>
            <th>購入者</th>
            <th>注文個数</th>
            <th>注文ステータス</th>
          </tr>
        <tbody>
          <% @orders.each do |order| %>
          <tr>
            <th><%= link_to order.create_at, admins_order(order.id) %></th>
            <th><%= order.end_user.name %></th>
            <th><%= order.count %></th>
            <th><%= order.status %></th>
          </tr>
          <% end %>
        </tbody>
        </thead>
      </table>
</div>