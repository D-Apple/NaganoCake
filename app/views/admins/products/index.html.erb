<h2>商品一覧</h2>
  <table class="table">
    <tr>
      <th>商品ID</th>
      <th>商品名</th>
      <th>税抜価格</th>
      <th>ジャンル</th>
      <th>ステータス</th>
    </tr>
    <% @products.each do |product| %>
    <tr>
      <th><%= product.id %></th>
      <th><%= link_to product.name %></th>
      <th><%= product.price %></th>
      <th><%= product.genre_id.name %></th>
      <th><%= product.is_status %></th>
    </tr>
  </table>